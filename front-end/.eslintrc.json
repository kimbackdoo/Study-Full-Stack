{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended"],
    "plugins": ["@typescript-eslint"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "parser": ["@typescript-eslint/parser"],
    "rules": {
        "no-unused-vars": "error",
        "no-console": "off"
    }
}
